{
   "compilerOptions": {
      "allowJs": true, // include .js and .jsx in project context (defaults to only .ts and .tsx)
      "target": "es5",
      "lib": [ "dom", "es5" ],
      "removeComments": true,
      "pretty": false,
      "alwaysStrict": true,
      "outFile": "../../bundles/unievents.es5.js" //compile down to ES5 for browser compatibility

   },
   "compileOnSave": true,

   "typeAcquisition": {
      "enable": true, // Defaulted to "false" with a tsconfig. Enables better IntelliSense in JS.
      "include": [ "../../lib/jquery" ], // Specific libs to fetch .d.ts files that weren't picked up by ATA
      "exclude": [ "node" ] // Specific libs to not fetch .d.ts files for
   },

   "files": [
      "../../lib/jquery-autocomplete/jquery.autocomplete.js",
      "../../lib/flatpickr/flatpickr.js",
      "../../lib/taggle/taggle.js",
      "unievents.js",
      "page-Account.js",
      "page-ApiTest.js",
      "page-CreateEvent.js",
      "page-Index.js",
      "page-Login.js",
      "page-SignUp.js"


   ] // list of explicit files to include in the project context. Highest priority.

   //"include": [], // list of folders or glob patterns to include in the project context.

}